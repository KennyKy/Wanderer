<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wanderer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Trabalho_01">
        <activity
            android:name="com.example.wanderer.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.wanderer.CreateAccount">
        </activity>

        <activity
            android:name="com.example.wanderer.Travel">
        </activity>

        <activity
            android:name="com.example.wanderer.Fuel">
        </activity>

        <activity
            android:name="com.example.wanderer.Airfare">
        </activity>

        <activity
            android:name="com.example.wanderer.Snack">
        </activity>

        <activity
            android:name="com.example.wanderer.Accommodation">
        </activity>

        <activity
            android:name="com.example.wanderer.Entertainment">
        </activity>

        <activity
            android:name="com.example.wanderer.TravelList">
        </activity>
    </application>

</manifest>